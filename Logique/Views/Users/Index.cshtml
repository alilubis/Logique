@model IEnumerable<Logique.Models.Entities.User>
@inject IHttpContextAccessor HttpContextAccessor
@{
    ViewBag.Title = "Dashboard Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string email = HttpContextAccessor.HttpContext.Session.GetString("email");
    string lastname = HttpContextAccessor.HttpContext.Session.GetString("lastname");
}

<h2>Users</h2>

Hello @lastname from email : @email

<p>
    @Html.ActionLink("Registration", "Register")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
        </tr>
    }

</table>
<p>
    @Html.ActionLink("Logout", "Logout")
</p>
